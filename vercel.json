{
  "routes": [
    {
      "src": "/(.*)\\.html",
      "dest": "/public/$1.html"
    },
    {
      "src": "/(.*)\\.js",
      "dest": "/public/$1.js"
    }

  ],
  "env": {
    "FLASK_APP": "server.py",
    "FLASK_ENV": "production"
  },
  "builds": [
    {
      "src": "*.py",
      "use": "@vercel/python"
    }
  ]
}
